{
    "name": "dghyse/fractal-cms",
    "description": "⚠️ OBSOLETE",
    "abandoned": true,
    "replace": {
        "webcraftdg/fractal-cms-content": "*"
    },
    "suggest": {
        "webcraftdg/fractal-cms-content": "Replacement package. Install this instead."
    },
    "homepage": "https://github.com/dghyse/fractal-cms",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dghyse/fractal-cms/issues"
    },
    "authors": [
        {
            "name": "David Ghyse",
            "email": "davidg@webcraftdg.fr"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-intl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "vlucas/phpdotenv": "~5.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-redis": "~2.0",
        "bower-asset/bootstrap": "~5.0",
        "league/flysystem": "^3.25"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "codeception/codeception": "^5.3",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "^2.0",
        "codeception/module-db": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "fractalCms\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": []
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
